{
  "name": "influxdata/influxdb-client-php",
  "description": "InfluxDB (v2+) Client Library for PHP",
  "keywords": [
    "InfluxDB"
  ],
  "version": "0.0.1-alpha",
  "homepage": "https://www.github.com/influxdata/influxdb-client-php",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.12",
    "phpspec/phpspec": "^5.0",
    "phpunit/phpunit": "^7.4",
    "squizlabs/php_codesniffer": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "InfluxDB2\\": "src/InfluxDB2",
      "InfluxDB2Generated\\": "src/InfluxDB2Generated/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InfluxDB2\\": "test/InfluxDB2",
      "InfluxDB2Generated\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests",
    "test-ci": "vendor/bin/phpunit tests --log-junit build/junit.xml -v --coverage-html=build/coverage-report"
  }

}
